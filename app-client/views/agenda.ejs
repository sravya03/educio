<html>
<head>
	<title>Educio</title>
	<link rel="stylesheet" type="text/css" href="/agenda.css">
	<link rel="stylesheet" type="text/css" href="/bootstrap-calendar/css/calendar.css">
	<link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css">
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
  	<script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>
</head>
<body>

<div id="header" class="row">
	<%- include partials/sidebarTabs.ejs %>
</div>

<div id="main-content" class="row">
	<div class="r col-md-1" style="background-color : red;"></div>
	<div class="r col-md-1" style="background-color : blue;"></div>
	<div class="r col-md-1" style="background-color : yellow;"></div>
	<div class="r col-md-1" style="background-color : green;"></div>
	<div class="r col-md-1" style="background-color : grey;"></div>
	<div class="r col-md-1" style="background-color : white;"></div>
	<div class="r col-md-1" style="background-color : orange;"></div>
	<div class="r col-md-1" style="background-color : black;"></div>
	<div class="r col-md-1" style="background-color : #498503;"></div>
	<div class="r col-md-1" style="background-color : #189039;"></div>
	<div class="r col-md-1" style="background-color : #290348;"></div>
	<div class="r col-md-1" style="background-color : #904829;"></div>
</div>

<div id="date-row" class="row">
	<div id="today-date" class="col-md-offset-6 col-md-1"></div>
</div>

<div class="row">
	<div id="calendar" class="col-md-offset-3 col-md-7"></div>
</div>

<script type="text/javascript" src="/bootstrap-calendar/js/vendor/underscore-min.js"></script>
<script type="text/javascript" src="/bootstrap-calendar/js/calendar.js"></script>
<script type="text/javascript">
    var calendar = jQuery("#calendar").calendar(
        {
            tmpl_path: "/bootstrap-calendar/tmpls/",
            events_source: function () { return []; }
        });         
</script>
<script type="text/javascript" src="/calendar_functionality.js"></script>
</body>
</html>